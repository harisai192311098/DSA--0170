#include <iostream>
using namespace std;
int main() {
    int size, sum = 0;
    cout << "Enter the size of the array: ";
    cin >> size;
    int arr[size];
    cout << "Enter the elements of the array: ";
    for (int i = 0; i < size; i++) {
        cin >> arr[i];
        sum += arr[i];
    }
    cout << "The sum of elements in the array is: " << sum << endl;
    return 0;
}
