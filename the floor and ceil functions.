#include <iostream>
#include <cmath>
using namespace std;
int main() {
    double num;
    cout << "Enter a floating-point number: ";
    cin >> num;
    int roundedDown =floor(num);
    int roundedUp =ceil(num);
    cout << "Rounded down to nearest integer: " << roundedDown <<endl;
    cout << "Rounded up to nearest integer: " << roundedUp << endl;
    return 0;
}
